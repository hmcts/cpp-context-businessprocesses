{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/listing/courts/public.listing.hearing-listed.json",
  "type": "object",
  "properties": {
    "hearingId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "hearingType": {
      "type": "string"
    },
    "caseUrns": {
      "description": "Prosecution cases to be added for hearing ",
      "type": "array",
      "minItems": 1,
      "items": {
        "properties": {
          "caseURN": {
            "type": "string"
          }
        },
        "required": [
          "caseURN"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "hearingId",
    "hearingType",
    "caseUrns"
  ],
  "additionalProperties": false
}