{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/bpm/json/schemas/bpm-definitions.json",
  "definitions": {
    "submissionStatus": {
      "type": "string",
      "enum": [
        "PENDING",
        "FAILED",
        "REJECTED",
        "SUCCESS",
        "SUCCESS_WITH_WARNINGS"
      ]
    },
    "channel": {
      "id": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/channel.json",
      "type": "string",
      "enum": [
        "SPI",
        "CPPI",
        "MCC"
      ]
    },
    "nationalInsuranceNumber": {
      "type": "string",
      "pattern": "^(?!BG)(?!GB)(?!NK)(?!KN)(?!TN)(?!NT)(?!ZZ)[A-CEGHJ-PR-TW-Z][A-CEGHJ-NPR-TW-Z][0-9][0-9][0-9][0-9][0-9][0-9][A-D]$"
    },

    "isoDate": {
      "type": "string",
      "pattern": "^\\d{4}-[01]\\d-[0-3]\\d$"
    }
  }
}